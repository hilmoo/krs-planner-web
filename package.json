{
  "name": "krs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "css": "mantine-reduce-css"
  },
  "dependencies": {
    "@mantine/code-highlight": "^8.3.13",
    "@mantine/core": "^8.3.13",
    "@mantine/hooks": "^8.3.13",
    "@mantine/notifications": "^8.3.13",
    "match-sorter": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "shiki": "^3.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@iconify-json/material-symbols": "^1.2.30",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "mantine-reduce-css": "^2.3.6",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "unplugin-icons": "^22.2.0",
    "vite": "^7.0.4"
  },
  "mantineReduceCss": {
    "target": [
      "src/**/*.tsx"
    ],
    "globalCss": true,
    "outputPath": "src/index.css",
    "extensions": {
      "CodeHighlight": true,
      "NotificationsSystem": true,
      "Spotlight": false,
      "Carousel": false,
      "Dropzone": false,
      "NavigationProgress": false,
      "ModalsManager": false,
      "RichTextEditor": false
    },
    "extend": []
  }
}
